<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1100.0" styleClass="_background" stylesheets="@styles/ListView.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.ListViewController">
   <children>
      <AnchorPane layoutX="759.0" layoutY="110.0" onInputMethodTextChanged="#onTypeInCombo" prefHeight="642.0" prefWidth="324.0" styleClass="_pane" />
      <ListView fx:id="_nameListView" layoutX="34.0" layoutY="118.0" onInputMethodTextChanged="#onTypeInCombo" onMouseClicked="#onClick" prefHeight="634.0" prefWidth="701.0" styleClass="_nameListView" stylesheets="@styles/ListView.css" />
      <Label alignment="TOP_LEFT" layoutX="34.0" layoutY="45.0" onInputMethodTextChanged="#onTypeInCombo" prefHeight="58.0" prefWidth="383.0" styleClass="_nameData" stylesheets="@styles/ListView.css" text="NAMES" textAlignment="CENTER" />
      <Button fx:id="_practiceButton" layoutX="766.0" layoutY="122.0" mnemonicParsing="false" onAction="#practiceButton" onInputMethodTextChanged="#onTypeInCombo" prefHeight="148.0" prefWidth="310.0" styleClass="_button" stylesheets="@styles/ListView.css" text="PRACTICE" />
      <Button layoutX="766.0" layoutY="311.0" mnemonicParsing="false" onAction="#clearSelection" onInputMethodTextChanged="#onTypeInCombo" prefHeight="148.0" prefWidth="310.0" styleClass="_button" stylesheets="@styles/ListView.css" text="CLEAR" />
        <ToggleButton fx:id="_randomButton" layoutX="921.0" layoutY="507.0" onAction="#onRandom" onInputMethodTextChanged="#onTypeInCombo" prefHeight="106.0" prefWidth="155.0" styleClass="_sortingButton" stylesheets="@styles/ListView.css" text="Random" />
        <ToggleButton fx:id="_sortedButton" layoutX="766.0" layoutY="507.0" onAction="#onSort" onInputMethodTextChanged="#onTypeInCombo" prefHeight="106.0" prefWidth="155.0" styleClass="_sortingButton" stylesheets="@styles/ListView.css" text="Sort" />
      <Button layoutX="962.0" layoutY="678.0" mnemonicParsing="false" onAction="#goBack" onInputMethodTextChanged="#onTypeInCombo" prefHeight="58.0" prefWidth="110.0" styleClass="_backButton" text="Back" />
      <Button layoutX="768.0" layoutY="678.0" mnemonicParsing="false" onAction="#importText" onInputMethodTextChanged="#onTypeInCombo" prefHeight="58.0" prefWidth="110.0" styleClass="_backButton" text="Import" />
      <ComboBox fx:id="_searchBox" editable="true" layoutX="170.0" layoutY="45.0" onInputMethodTextChanged="#onTypeInCombo" prefHeight="40.0" prefWidth="490.0" promptText="Combine names..." />
   </children>
</AnchorPane>
