<AnchorPane maxHeight="800.0" maxWidth="1100.0" prefHeight="800.0" prefWidth="1100.0" styleClass="_background" stylesheets="@styles/Practice.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.UserRecordingsController">
   <children>
      <Pane layoutX="61.0" layoutY="76.0" prefHeight="411.0" prefWidth="979.0" styleClass="_pane" />
      <Label fx:id="_nameDisplayed" alignment="CENTER" layoutX="200.0" layoutY="94.0" prefHeight="272.0" prefWidth="700.0" styleClass="_nameDisplayed" stylesheets="@styles/Practice.css" text="Text goes here" textAlignment="CENTER">
         <font>
            <Font size="93.0" />
         </font>
      </Label>
      <Pane layoutX="166.0" layoutY="586.0" prefHeight="137.0" prefWidth="762.0" styleClass="_pane" />
      <HBox fx:id="_buttonsHBox" alignment="CENTER" layoutX="200.0" layoutY="600.0" prefHeight="107.0" prefWidth="700.0">
         <children>
            <Button fx:id="_deleteButton" mnemonicParsing="false" onAction="#deleteUserRecording" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" stylesheets="@styles/Practice.css" text="Delete">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_compareButton" mnemonicParsing="false" onAction="#compare" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Compare">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_backButton" mnemonicParsing="false" onAction="#goBack" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Back">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button fx:id="_dbVersionButton" layoutX="740.0" layoutY="407.0" mnemonicParsing="false" onAction="#playDatabaseRecording" prefHeight="62.0" prefWidth="230.0" styleClass="_audioButton" stylesheets="@styles/Practice.css" text="Database Recording" />
      <ProgressBar fx:id="_progressBar" layoutX="197.0" layoutY="506.0" prefHeight="47.0" prefWidth="700.0" progress="0.0" />
      <Label layoutX="372.0" layoutY="14.0" styleClass="_title" text="USER RECORDINGS" />
      <Slider fx:id="_volumeSlider" layoutX="999.0" layoutY="572.0" orientation="VERTICAL" prefHeight="130.0" prefWidth="50.0" />
      <Label layoutX="1004.0" layoutY="715.0" text="Volume" textFill="WHITE" />
      <ComboBox fx:id="_dropdown" layoutX="380.0" layoutY="407.0" prefHeight="62.0" prefWidth="330.0" promptText="Versions" styleClass="_dropdown" stylesheets="@styles/Practice.css" />
      <Button fx:id="_userVersionButton" layoutX="120.0" layoutY="407.0" mnemonicParsing="false" onAction="#playUserRecording" prefHeight="62.0" prefWidth="230.0" styleClass="_audioButton" stylesheets="@styles/Practice.css" text="User Recording" />
   </children>
</AnchorPane>
