<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1100.0" styleClass="_background" stylesheets="@styles/Practice.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.PracticeController">
   <children>
      <Pane layoutX="61.0" layoutY="77.0" prefHeight="411.0" prefWidth="979.0" styleClass="_pane" />
      <Label fx:id="_nameDisplayed" alignment="CENTER" layoutX="200.0" layoutY="94.0" prefHeight="272.0" prefWidth="700.0" styleClass="_nameDisplayed" stylesheets="@styles/Practice.css" text="Text goes here" textAlignment="CENTER">
         <font>
            <Font size="93.0" />
         </font>
      </Label>
      <Button fx:id="_prevButton" layoutX="82.0" layoutY="177.0" mnemonicParsing="false" onAction="#prevName" prefHeight="26.0" prefWidth="110.0" styleClass="_changeButton" stylesheets="@styles/Practice.css" text="&lt;" />
      <Button fx:id="_nextButton" layoutX="909.0" layoutY="186.0" mnemonicParsing="false" onAction="#nextName" prefHeight="56.0" prefWidth="104.0" styleClass="_changeButton" stylesheets="@styles/Practice.css" text="&gt;" />
      <ComboBox fx:id="_dropdown" layoutX="200.0" layoutY="407.0" prefHeight="12.0" prefWidth="594.0" promptText="Versions" styleClass="_dropdown" stylesheets="@styles/Practice.css" />
      <Pane layoutX="166.0" layoutY="586.0" prefHeight="137.0" prefWidth="762.0" styleClass="_pane" />
      <HBox fx:id="_recordHBox" alignment="CENTER" layoutX="200.0" layoutY="600.0" prefHeight="107.0" prefWidth="700.0">
         <children>
            <Button fx:id="_recordButton" mnemonicParsing="false" onAction="#recordAudio" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" stylesheets="@styles/Practice.css" text="Record">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_rateButton" mnemonicParsing="false" onAction="#rate" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Rate Bad">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_backButton" mnemonicParsing="false" onAction="#goBack" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Back">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button fx:id="_listenButton" layoutX="805.0" layoutY="407.0" mnemonicParsing="false" onAction="#playAudio" prefHeight="62.0" prefWidth="104.0" styleClass="_audioButton" stylesheets="@styles/Practice.css" text="Listen" />
      <ProgressBar fx:id="_progressBar" layoutX="197.0" layoutY="506.0" prefHeight="47.0" prefWidth="700.0" progress="0.0" />
      <HBox fx:id="_confirmationHBox" layoutX="200.0" layoutY="600.0" prefHeight="107.0" prefWidth="700.0">
         <children>
            <Button mnemonicParsing="false" onAction="#keepRecording" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Keep">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#playBackAudioOfRecording" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Replay">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#cancelAudioRecording" prefHeight="107.0" prefWidth="240.0" styleClass="_selectionButton" text="Cancel">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label layoutX="457.0" layoutY="14.0" styleClass="_title" text="PRACTICE" />
   </children>
</AnchorPane>
