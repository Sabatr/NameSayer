<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.PracticeController">
   <children>
      <Label fx:id="_nameDisplayed" alignment="CENTER" layoutX="245.0" layoutY="61.0" prefHeight="186.0" prefWidth="610.0" text="Text goes here" textAlignment="CENTER">
         <font>
            <Font size="93.0" />
         </font>
      </Label>
      <Button fx:id="_prevButton" layoutX="78.0" layoutY="154.0" mnemonicParsing="false" onAction="#prevName" text="Back" />
      <Button fx:id="_nextButton" layoutX="925.0" layoutY="154.0" mnemonicParsing="false" onAction="#nextName" text="Next" />
      <ComboBox layoutX="429.0" layoutY="290.0" prefWidth="150.0" promptText="Versions" />
      <HBox alignment="CENTER" layoutX="341.0" layoutY="536.0" prefHeight="100.0" prefWidth="419.0">
         <children>
            <Button mnemonicParsing="false" text="Try it out">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="rate">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#goBack" prefHeight="31.0" prefWidth="72.0" text="back">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button layoutX="591.0" layoutY="290.0" mnemonicParsing="false" text="listen" />
   </children>
</AnchorPane>
