<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1100.0" styleClass="_background" stylesheets="@styles/Practice.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.PracticeController">
   <children>
      <Label fx:id="_nameDisplayed" alignment="CENTER" layoutX="200.0" layoutY="83.0" prefHeight="272.0" prefWidth="700.0" styleClass="_nameDisplayed" stylesheets="@styles/Practice.css" text="Text goes here" textAlignment="CENTER">
         <font>
            <Font size="93.0" />
         </font>
      </Label>
      <Button fx:id="_prevButton" layoutX="84.0" layoutY="154.0" mnemonicParsing="false" onAction="#prevName" prefHeight="130.0" prefWidth="90.0" styleClass="_changeButton" stylesheets="@styles/Practice.css" text="&lt;" />
      <Button fx:id="_nextButton" layoutX="909.0" layoutY="154.0" mnemonicParsing="false" onAction="#nextName" prefHeight="130.0" prefWidth="90.0" styleClass="_changeButton" stylesheets="@styles/Practice.css" text="&gt;" />
      <ComboBox fx:id="_dropdown" layoutX="200.0" layoutY="372.0" prefHeight="42.0" prefWidth="594.0" promptText="Versions" styleClass="_dropdown" stylesheets="@styles/Practice.css" />
      <HBox alignment="CENTER" layoutX="200.0" layoutY="600.0" prefHeight="107.0" prefWidth="700.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="107.0" prefWidth="232.0" styleClass="_selectionButton" stylesheets="@styles/Practice.css" text="Record">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="_rateButton" mnemonicParsing="false" onAction="#rate" prefHeight="107.0" prefWidth="254.0" styleClass="_selectionButton" text="Rate">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#goBack" prefHeight="107.0" prefWidth="226.0" styleClass="_selectionButton" text="Back">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button layoutX="805.0" layoutY="372.0" mnemonicParsing="false" onAction="#playAudio" prefHeight="56.0" prefWidth="104.0" styleClass="_audioButton" stylesheets="@styles/Practice.css" text="Listen" />
   </children>
</AnchorPane>
